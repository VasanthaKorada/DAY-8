class Solution {
public:
    vector<vector<int>> generate(int numRows) {
       if(numRows==1)
        return {{1}};
    vector<vector<int>>arr=generate(numRows-1);
    vector<int>prev;
    vector<int>curr;
    curr.push_back(1);
   prev=arr.back();
    for(int i=1;i<prev.size();i++){
        curr.push_back(prev[i-1]+prev[i]);
    }
curr.push_back(1);
arr.push_back(curr);
return arr;
    }
};
